
<%- partial('./partial/sidemenu.ejs') %>
<section class="content aboutus flex-lg-grow-1">
  <h5 class="main-heading1">教練<span class="highlight">資訊</span></h5>
<div class="container-fluid">
<div class="row text-center">
  
<table class="table">
  <% coach.forEach(function(model) { %>
      <% if (model.Disciplines5=="TRA"){ %>
        <div class="container">
          <div class="card" style="width:250px">
            <img class="card-img-top" src="<%=req.session.data.Personal_photo%>" alt="Card image" style="width:100%">
            <div class="card-body">
              <h5 class="card-title"><a href="/coach/coachform_detail/<%=model.id%>"> <%= model.ChiName %>教練</a></h5>
            </div>
          </div>
          <br>
  <%}%>
      <% }); %>
  </div>
</div>
</section>
