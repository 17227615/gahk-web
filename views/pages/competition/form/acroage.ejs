                <label>出生日期 Date of Birth (dd/mm/yyyy):</label>
                <input type="date" class="form-group" name="Acroage[birthday2]" id="birth2" required>
                <br>
            </div>
            <div>
                <label>身份證號碼 ID Card Number:</label>
                <input type="text" class="form-group" name="Acroage[idNo2]" placeholder=" A123456(7)" required>
            </div>
            <div>
                <label>聯絡電話 Contact No.:</label>
                <input type="tel" pattern=[0-9]{8} class="form-group" name="Acroage[contactNo2]" placeholder="12345678"
                    required>
            </div>
            <div>
                <label>電郵 Email Address:</label>
                <input type="email" class="form-group" name="Acroage[email2]" placeholder="example@gmail.com"
                    style="width: 200pt;" required>
            </div>
            <div>
                <label>聯絡地址 Contact Address:</label>
                <input type="text" class="form-group" name="Acroage[address2]" style="width: 350pt;" required>
            </div>
            <div>
